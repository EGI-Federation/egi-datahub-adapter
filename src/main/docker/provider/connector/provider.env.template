# SPRING_JPA_DATABASEPLATFORM=org.hibernate.dialect.PostgreSQLDialect

####################################################################################################
## HTTP/S                                                                                         ##
####################################################################################################

SERVER_SSL_ENABLED=true
SERVER_SSL_KEY_STORE=/app/conf/provider-keystore.p12
SERVER_SSL_KEY_STORE_PASSWORD=<put-keystore-password-here>
SERVER_SSL_KEY_STORE_TYPE=PKCS12
# SERVER_SSL_KEY_ALIAS=1
