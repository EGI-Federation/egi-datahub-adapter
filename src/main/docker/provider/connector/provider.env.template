####################################################################################################
## Dataspace Connector                                                                            ##
####################################################################################################

## General configuration
TITLE=EGI DataHub Connector
PROJECT_DESCRIPTION=Provides controlled access to data in EGI DataHub
ORGANIZATION_NAME=EGI Foundation
LICENSE=Open Data
LICENSE_URL=http://www.opendefinition.org/okd/
CONTACT_URL=https://www.egi.eu/contact/
CONTACT_EMAIL=support@egi.eu
CURATOR=mailto:levente.farkas@egi.eu
MAINTAINER=mailto:levente.farkas@egi.eu

# The component certificate (included in file provider.crt)
COMPONENT_CERTIFICATE=""

# The ID used when requesting the component certificate
COMPONENT_ID=urn:euh4d:ids:connectors:EGI-DataHub

# The public endpoint of the connector
COMPONENT_ENDPOINT=https://localhost:8081

# The deploy mode (either PRODUCTIVE_DEPLOYMENT or TEST_DEPLOYMENT)
DEPLOY_MODE=PRODUCTIVE_DEPLOYMENT

## Spring security (API access)
SPRING_SECURITY_USER_NAME=admin
SPRING_SECURITY_USER_PASSWORD=password
# SPRING_SECURITY_APP_NAME=app
# SPRING_SECURITY_APP_PASSWORD=password

## Bootstrap
# BOOTSTRAP_PATH=.
# BOOTSTRAP_ENABLED=false

## OpenAPI
SPRINGDOC_SWAGGER_UI_PATH=/api/docs
SPRINGDOC_SWAGGER_UI_DISABLE_SWAGGER_DEFAULT_URL=true
# SPRINGDOC_SWAGGER_UI_DOC_EXPANSION=none
# SPRINGDOC_SWAGGER_UI_OPERATIONSSORTER=alpha
# SPRINGDOC_SWAGGER_UI_TAGSSORTER=alpha
# SPRINGDOC_WRITER_WITH_DEFAULT_PRETTY_PRINTER=true

# Management information, list of "info", "loggers", "logfile"
# https://docs.spring.io/spring-boot/docs/current/reference/html/actuator.html
# To check the connector version, certificate expiration status, and DAT info, add "info"
#   The details will be available at endpoint /actuator/info
# To modify logging levels at runtime, add "loggers"
#   Available loggers and their current logging level will be exposed at endpoint /actuator/loggers
# To access the logfiles remotely, add "logfile"
#   The logfile will be available at endpoint /actuator/logfile
# MANAGEMENT_ENDPOINTS_ENABLED_BY_DEFAULT=true
# MANAGEMENT_ENDPOINTS_WEB_EXPOSURE_INCLUDE=info
# MANAGEMENT_ENDPOINT_LOGGERS_ENABLED=true
# MANAGEMENT_ENDPOINT_LOGFILE_ENABLED=true
# MANAGEMENT_ENDPOINT_LOGFILE_EXTERNAL_FILE=./log/dataspaceconnector.log

####################################################################################################
## IDS Properties                                                                                 ##
####################################################################################################

## Connector configuration
CONFIGURATION_FORCE_UPDATE=true
CONFIGURATION_PATH=/app/conf/config.json
CONFIGURATION_TRUSTSTOREPASSWORD=euhubs4data
CONFIGURATION_KEYSTOREPASSWORD=<put-keystore-password-here>
# CONFIGURATION_KEYALIAS=1

## InfoModel version (determines the connector versions we can consume from)
# Use 4.2.7 for v7.x or later
# Use 4.2.6 for v6.5 or later
OUTBOUND_VERSION=4.2.7

## The DAPS
#DAPS_URL=https://daps.aisec.fraunhofer.de
#DAPS_TOKEN_URL=https://daps.aisec.fraunhofer.de/v2/token

DAPS_URL=https://daps.euh4d.dataspac.es
DAPS_TOKEN_URL=https://daps.euh4d.dataspac.es/v2/token

## The clearing house (if not defined, will not use one)
# CLEARING_HOUSE_URL=https://ch-ids.aisec.fraunhofer.de

## Policy handling (framework can be INTERNAL or MYDATA)
POLICY_NEGOTIATION=true
POLICY_ALLOW_UNSUPPORTED_PATTERNS=false
POLICY_FRAMEWORK=INTERNAL

## Camel routes
CAMEL_TRUSTSTORE_PATH=/app/conf/euh4d-truststore.p12

## IDSCP
IDSCP2_ENABLED=false
# IDSCP2_KEYSTORE=/app/conf/provider-keystore.p12
# IDSCP2_TRUSTSTORE=/app/conf/euh4d-truststore.p12

####################################################################################################
